<p id="notice"><%= notice %></p>

<h1>Listing Calificacions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Foto</th>
      <th>Puntuacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calificacions.each do |calificacion| %>
      <tr>
        <td><%= calificacion.user.email %></td>
        <td><%= calificacion.foto.id %></td>
        <td><%= calificacion.puntuacion %></td>
        <td><%= link_to 'Eliminar', [calificacion.foto, calificacion] , method: :delete, data: { confirm: 'Estas seguro?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


